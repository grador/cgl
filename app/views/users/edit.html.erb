<h1>Отредактируйте атрибуты пользователя</h1>
<%= form_for @user do |f| %>
<%= render partial: 'shared/user_form', locals: { f: f } %>
    <ul class="form_nav">
      <li class="button"><%= f.submit 'Сохранить' %></li>
      <li class="button"><%= link_to 'Отменить', action: 'show' %></li>
    </ul>
<% end %>
